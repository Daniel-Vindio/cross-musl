program="grep"
version="$VER_grep"
release="1"
#arch=""

tarname=${program}-${version}.tar.xz

description="

The Grep package contains programs for searching through files..
---
"


build() {

unpack "${tardir}/$tarname"
cd "$srcdir"

echo -e "\nCreaciÃ³n paquete Qi $program \n$(date)" >> $FILE_BITACORA

./configure \
--prefix=/usr \
--bindir=/bin

make

make DESTDIR=${destdir} install
}











