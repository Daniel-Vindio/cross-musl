program="texinfo"
version="$VER_texinfo"
release="1"
#arch=""

tarname=${program}-${version}.tar.xz

description="

The Texinfo package contains programs for reading, writing, and 
converting info pages.
---
"


build() {

unpack "${tardir}/$tarname"
cd "$srcdir"

echo -e "\nCreaciÃ³n paquete Qi $program \n$(date)" >> $FILE_BITACORA

./configure \
--prefix=/usr \
--disable-static

make

make DESTDIR=${destdir} install
}











